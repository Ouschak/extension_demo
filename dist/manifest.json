{
  "manifest_version": 3,
  "name": "SocialShield",
  "version": "2.2.19",
  "description": "Protect children from online hate speech with AI-powered detection. Filters toxic comments locally on Instagram to keep kids safe.",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "https://www.instagram.com/*",
    "http://161.97.107.249:8000/*"
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/index.ts-CKVtIOu8.js"
      ],
      "matches": [
        "https://www.instagram.com/*"
      ],
      "run_at": "document_idle",
      "css": [
        "assets/index-Ce6ywIx7.css"
      ]
    }
  ],
  "action": {
    "default_popup": "src/popup/index.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "https://www.instagram.com/*"
      ],
      "resources": [
        "assets/index.ts-CKVtIOu8.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
